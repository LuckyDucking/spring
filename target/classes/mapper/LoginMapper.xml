<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.LoginMapper">
    <cache flushInterval="60000" eviction="LRU" size="1024" readOnly="false"/>
    <resultMap id="LoginResultMap" type="Login">
        <id column="login_log_id" property="loginLogId"/>
        <result column="user_id" property="userId"/>
        <result column="ip" property="ip"/>
        <result column="login_datetime" property="loginDatetime"/>
    </resultMap>
    <select id="selectLogin" parameterType="Login" resultMap="LoginResultMap">
        select * from t_login_log
        <where>
            <if test="login_log_id!=null">login_log_id=#{login_log_id}</if>
            <if test="user_id!=null">user_id=#{user_id}</if>
            <if test="ip!=null">ip=#{ip}</if>
            <if test="login_datetime!=null">login_datetime=#{login_datetime}</if>
        </where>
    </select>
</mapper>